@{
    ViewData["Title"] = "Backend Service";
}

<div class="text-center">
    <h1 class="display-4">Backend Service Responses</h1>

    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Response from UserData call</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" value="@Model.UserData" disabled></input>
        </div>
    </div>

    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Response from AdminData call</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" value="@Model.AdminData" disabled></input>
        </div>
    </div>

</div>